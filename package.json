{
  "name": "mint-nft",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=dev node src/bin/www.js",
    "estart": "cross-env NODE_ENV=externaldev nodemon -e js,ejs src/bin/www.js",
    "pstart": "cross-env NODE_ENV=production nodemon -e js,ejs src/bin/www.js",
    "api-docs": "swagger-cli bundle ./src/apidoc/openapi.yaml --outfile build/swagger.yaml --type yaml"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/**",
      "**/docs/**"
    ]
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.6",
