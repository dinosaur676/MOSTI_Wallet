    "@openzeppelin/contracts": "^4.7.3",
    "@truffle/hdwallet-provider": "^2.0.14",
    "amqplib": "^0.10.3",
    "cookie-parser": "~1.4.4",
    "d3-mitch-tree": "^1.1.2",
    "dayjs": "^1.11.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.2",
    "ejs": "^3.1.8",
    "eslint-plugin-import": "^2.25.2",
    "ethereumjs-common": "^1.5.2",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-wallet": "^1.0.2",
    "express": "~4.16.1",
    "foundation-sites": "^6.7.5",
    "http-errors": "~1.6.3",
