{
  "root": true,
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "plugins": [],
  "extends": ["eslint:recommended","airbnb-base","plugin:prettier/recommended" ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "off"
    ],
    "no-use-before-define": ["error", {"functions": false, "classes": false}],
    "no-nested-ternary": "off",
    "import/no-dynamic-require": "off",
    "no-return-await": "off"
  }
}
